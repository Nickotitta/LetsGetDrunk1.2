<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.nick.letsgetdrunk12.gamesWithCards">

    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="97dp"
        android:layout_height="102dp"
        android:layout_alignEnd="@+id/imageButton"
        android:layout_centerVertical="true"
        android:background="@null"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/comming" />

    <ImageButton
        android:id="@+id/ringOfFireButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="37dp"
        android:layout_marginTop="59dp"
        android:radius="35dp"
        app:srcCompat="@mipmap/ic_launcher"
        tools:layout_editor_absoluteX="43dp"
        tools:layout_editor_absoluteY="50dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginLeft="22dp"
        android:layout_marginStart="22dp"
        android:layout_marginTop="14dp"
        android:text="Ring of Fire"
        android:textSize="25sp"
        tools:layout_editor_absoluteX="42dp"
        tools:layout_editor_absoluteY="16dp" />

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="109dp"
        android:layout_height="97dp"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/ringOfFireButton"
        android:layout_marginEnd="75dp"
        android:background="@null"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/comming"
        />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/textView"
        android:layout_marginEnd="41dp"
        android:text="Comming Soon "
        android:textSize="25sp" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="97dp"
        android:layout_height="102dp"
        android:layout_alignStart="@+id/ringOfFireButton"
        android:layout_centerVertical="true"
        android:background="@null"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/comming" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="171dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="14dp"
        android:layout_marginTop="191dp"
        android:text="Comming Soon "
        android:textSize="25sp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/textView5"
        android:layout_marginEnd="21dp"
        android:text="Comming Soon "
        android:textSize="25sp" />
</RelativeLayout>

