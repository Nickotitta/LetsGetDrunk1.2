<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.nick.letsgetdrunk12.gamesWithTalks">

    <ImageButton
        android:id="@+id/iveNeverButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignEnd="@+id/textView3"
        android:layout_alignRight="@+id/textView3"
        android:layout_below="@+id/textView3"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="37dp"
        android:layout_marginTop="59dp"
        app:srcCompat="@mipmap/ic_launcher"
        tools:layout_editor_absoluteX="43dp"
        tools:layout_editor_absoluteY="50dp" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="32dp"
        android:layout_marginTop="13dp"
        android:text="@string/iveNeverText"
        android:textSize="25sp"
        tools:layout_editor_absoluteX="29dp"
        tools:layout_editor_absoluteY="16dp" />


    <ImageButton
        android:id="@+id/imageButton3"
        android:layout_width="97dp"
        android:layout_height="102dp"
        android:layout_alignEnd="@+id/textView4"
        android:layout_alignTop="@+id/imageButton2"
        android:background="@null"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/comming" />


    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="92dp"
        android:layout_height="95dp"
        android:layout_alignEnd="@+id/imageButton3"
        android:layout_alignTop="@+id/iveNeverButton"
        android:background="@null"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/comming" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignTop="@+id/textView3"
        android:layout_toEndOf="@+id/textView5"
        android:text="@string/Comming"
        android:textSize="25sp" />

    <ImageButton
        android:id="@+id/imageButton2"
        android:layout_width="97dp"
        android:layout_height="102dp"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/iveNeverButton"
        android:layout_marginTop="246dp"
        android:background="@null"
        android:scaleType="fitCenter"
        app:srcCompat="@drawable/comming" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="196dp"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="14dp"
        android:layout_marginTop="191dp"
        android:text="@string/Comming"
        android:textSize="25sp" />

    <TextView
        android:id="@+id/textView6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentEnd="true"
        android:layout_alignTop="@+id/textView5"
        android:layout_marginEnd="12dp"
        android:text="@string/Comming"
        android:textSize="25sp" />
</RelativeLayout>
